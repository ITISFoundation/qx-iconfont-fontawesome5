{
  "targets": [
    {
      "type": "source",
      "outputPath": "source-output"
    },
    {
      "type": "hybrid",
      "outputPath": "hybrid-output"
    },
    {
      "type": "build",
      "outputPath": "build-output"
    }
  ],
  "defaultTarget": "source",
  "serve": {
    "listenPort": 31754
  },
  "locales": [
    "en"
  ],
  "environment": {
    "qx.icontheme": "Tango"
  },
  "applications": [
    {
      "class": "iconbrowser.Application",
      "theme": "qx.theme.Simple",
      "name": "iconbrowser",
      "include": [
        "iconfont.fontawesome5.Load"
      ]
    }
  ]
}
